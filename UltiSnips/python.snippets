snippet bootstrap
from types import GeneratorType
def bootstrap(f, stack=[]):
    def wrappedfunc(*args, **kwargs):
        if stack:
            return f(*args, **kwargs)
        else:
            to = f(*args, **kwargs)
            while True:
                if type(to) is GeneratorType:
                    stack.append(to)
                    to = next(to)
                else:
                    stack.pop()
                    if not stack:
                        break
                    to = stack[-1].send(to)
            return to
    return wrappedfunc
endsnippet

snippet binPow
def binPow(n, r):
    c = 1
    while r > 0:
        if r & 1:
            r -= 1
            c *= n
        r = r // 2
        n = n ** 2
    return c
endsnippet

