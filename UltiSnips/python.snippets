import sys
from collections import deque as queue
sys.setrecursionlimit(10 ** 6)
class Graph(object):

    def __init__(self):
        self.neighbours = {}
    
    def __repr__(self):
        s = ''
        for i in self.neighbours:
            s += str(i)+': '+' '.join(map(str,self.neighbours[i])) + '\n'
        return s
    
    def add_node(self, node):
        self.neighbours[node] = set()
    
    def add_edge(self, edge):
        u, v = edge
        self.neighbours[u].add(v)
        self.neighbours[v].add(u)
    
    def dfs(self, node):
        visited[node] = 1
        for child in self.neighbours[node]:
            if visited[child] == 0:
                self.dfs(child)
    
    def bfs(self, node):
        q = queue()
        visited[node] = 1
        q.append(node)
        while q:
            cur = q.popleft()
            for child in self.neighbours[cur]:
                if visited[child] == 0:
                    q.append(child)
                    visited[child] = 1

g = Graph()
n, e = map(int, input().split())
for i in range(n):
    g.add_node(i+1)
for i in range(e):
    x, y = map(int, input().split())
    g.add_edge((x, y))
visited = [0] * (n + 1)
